{
  "name": "@eshop/auth-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node src/main.ts"
  },
  "nx": {
    "name": "@eshop/auth-service",
    "projectType": "application",
    "sourceRoot": "auth-service/src",
    "targets": {
      "build": {
        "options": {
          "cwd": "apps/auth-service",
          "args": ["--node-env=production"],
          "command": "webpack-cli build --config webpack.config.js"
        },
        "cache": true,
        "dependsOn": ["^build"],
        "inputs": ["production", "^production", {"externalDependencies": ["webpack-cli"]}],
        "outputs": ["{workspaceRoot}/dist/apps/auth-service"],
        "metadata": {
          "technologies": ["webpack"],
          "description": "Runs Webpack build",
          "help": {"command": "npx webpack-cli build --help", "example": {"options": {"json": "stats.json"}, "args": ["--profile"]}}
        },
        "syncGenerators": ["@nx/js:typescript-sync"],
        "executor": "nx:run-commands",
        "configurations": {},
        "parallelism": true
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "configurations": {
          "development": {
            "buildTarget": "@eshop/auth-service:build:development"
          },
          "production": {
            "buildTarget": "@eshop/auth-service:build:production"
          }
        }
      },
      "test": {
        "options": {
          "passWithNoTests": true
        }
      }
    }
  }
}
